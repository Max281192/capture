\\REACT\\Chapter 3\\capture\\src\\pages\\MovieDetail.js",["76"],"import React, {useState, useEffect} from 'react';\r\nimport styled from 'styled-components';\r\nimport {useHistory} from 'react-router-dom';\r\nimport {MovieState} from '../movieState';\r\n\r\n// Page animation\r\nimport {motion} from 'framer-motion';\r\nimport {pageAnimation} from '../animation';\r\n\r\nconst MovieDetail = () => {\r\n\r\n    const history = useHistory();\r\n    const url = history.location.pathname;\r\n    const [movies, setMovies] = useState(MovieState);\r\n    const [movie, setMovie] = useState(null);\r\n\r\n    //UseEffect\r\n    useEffect(() => {\r\n        const currentMovie = movies.filter((stateMovie) => stateMovie.url === url);\r\n        setMovie(currentMovie[0]);\r\n    }, [movies, url]);\r\n\r\n    return (\r\n        <>\r\n            {movie && (\r\n                <Details exit=\"exit\" variants={pageAnimation} initial=\"hidden\" animate=\"show\">\r\n\r\n                    <HeadLine>\r\n                        <h2>{movie.title}</h2>\r\n                        <img src={movie.mainImg} alt=\"main\"/>\r\n                    </HeadLine>\r\n\r\n                    <Awards>\r\n                        {movie.awards.map((award) => (\r\n                            <Award\r\n                                title={award.title}\r\n                                description={award.description}\r\n                                key={award.title}\r\n                            />\r\n                        ))}\r\n                    </Awards>\r\n\r\n                    <ImageDisplay>\r\n                        <img src={movie.secondaryImg} alt=\"secondary\"/>\r\n                    </ImageDisplay>\r\n\r\n                </Details>\r\n            )}\r\n        </>\r\n    )\r\n}\r\n\r\n//Style\r\nconst Details = styled(motion.div)`\r\n  color: white;\r\n`;\r\n\r\nconst HeadLine = styled.div`\r\n  min-height: 90vh;\r\n  padding-top: 20vh;\r\n  position: relative;\r\n\r\n  h2 {\r\n    position: absolute;\r\n    top: 8%;\r\n    left: 50%;\r\n    transform: translate(-50%, -8%);\r\n  }\r\n\r\n  img {\r\n    width: 100%;\r\n    height: 70vh;\r\n    object-fit: cover;\r\n  }\r\n`;\r\n\r\nconst Awards = styled.div`\r\n  min-height: 80vh;\r\n  margin: 5rem 10rem;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-around;\r\n`;\r\n\r\nconst AwardStyle = styled.div`\r\n  padding: 5rem;\r\n  \r\n  h3{\r\n    font-size: 2rem;\r\n  }\r\n  \r\n  p{\r\n    padding: 2rem 0;\r\n  }\r\n  \r\n  .line{\r\n    width: 100%;\r\n    background-color: #23d997;\r\n    height: 0.25rem;\r\n    margin: 1rem 0;\r\n  }\r\n`;\r\n\r\nconst ImageDisplay = styled.div`\r\n  min-height: 50vh;\r\n  \r\n  img{\r\n    width: 100%;\r\n    height: 100vh;\r\n    object-fit: cover;\r\n  }\r\n`;\r\n\r\n//Award Component\r\nconst Award = ({title, description}) => {\r\n    return (\r\n        <AwardStyle>\r\n            <h3>{title}</h3>\r\n            <div className=\"line\"></div>\r\n            <p>{description}</p>\r\n        </AwardStyle>\r\n    )\r\n}\r\n\r\nexport default MovieDetail;","C:\\Users\\Maxx\\Desktop\\REACT\\Chapter 3\\capture\\src\\movieState.js",[],"C:\\Users\\Maxx\\Desktop\\REACT\\Chapter 3\\capture\\src\\animation.js",[],"C:\\Users\\Maxx\\Desktop\\REACT\\Chapter 3\\capture\\src\\components\\Wave.js",[],"C:\\Users\\Maxx\\Desktop\\REACT\\Chapter 3\\capture\\src\\components\\Toggle.js",[],{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","replacedBy":"80"},{"ruleId":"81","severity":1,"message":"82","line":7,"column":16,"nodeType":"83","messageId":"84","endLine":7,"endColumn":27},{"ruleId":"81","severity":1,"message":"85","line":7,"column":29,"nodeType":"83","messageId":"84","endLine":7,"endColumn":34},{"ruleId":"81","severity":1,"message":"86","line":14,"column":20,"nodeType":"83","messageId":"84","endLine":14,"endColumn":29},"no-native-reassign",["87"],"no-negated-in-lhs",["88"],"no-unused-vars","'Description' is defined but never used.","Identifier","u